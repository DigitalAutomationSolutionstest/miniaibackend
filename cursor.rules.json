{
  "rules": {
    "**/.env": "ignore",
    "**/.env.*": "ignore",
    "**/.vercel/**": "ignore",
    "**/.next/**": "ignore",
    "**/node_modules/**": "ignore",
    "**/package-lock.json": "track",
    "**/pnpm-lock.yaml": "track",
    "**/app/api/**/route.ts": "track",
    "**/lib/**": "track",
    "**/components/**": "track",
    "next.config.js": "track",
    "vercel.json": "track",
    "tsconfig.json": "track",
    "package.json": "track",
    ".gitignore": "track",
    ".env.example": "track",
    "README.md": "track",
    "public/**": "track",
    "middleware.ts": "track",
    "tailwind.config.js": "track"
  },
  "meta": {
    "projectType": "nextjs",
    "api": true,
    "envManagement": "vercel",
    "deployment": "vercel",
    "language": "typescript"
  }
}
